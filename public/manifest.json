{
  "name": "my-own-words",
  "version": "1.0",
  "description": "Browser extension using Web3",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.bundle.js", "logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
